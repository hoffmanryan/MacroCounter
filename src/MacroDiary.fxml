<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="460.0" prefWidth="512.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MacroDiaryController">
   <children>
      <TextArea fx:id="areaDiary" editable="false" layoutX="35.0" layoutY="109.0" prefHeight="243.0" prefWidth="235.0" promptText="Select the date range you wish to view.">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor></TextArea>
      <Text layoutX="180.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Macro Diary">
         <font>
            <Font size="26.0" />
         </font>
      </Text>
      <Button fx:id="viewGraph" layoutX="60.0" layoutY="397.0" mnemonicParsing="false" onAction="#submitClicked" text="View Diary" />
      <Button fx:id="clearButton" layoutX="180.0" layoutY="397.0" mnemonicParsing="false" onAction="#clearButtonClicked" text="Clear" />
      <Button fx:id="backButton" layoutX="270.0" layoutY="397.0" mnemonicParsing="false" onAction="#returnBack" text="Back" />
      <DatePicker fx:id="startDate" layoutX="305.0" layoutY="133.0" promptText="Select Start Date" />
      <Button fx:id="viewGraph" layoutX="358.0" layoutY="397.0" mnemonicParsing="false" onAction="#viewGraph" text="View Graph" />
   </children>
</AnchorPane>
